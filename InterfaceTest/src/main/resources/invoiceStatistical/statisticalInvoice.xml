<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name = "统计各个票种发票开具情况">
    <test name = "正常统计各个票种开具情况">
        <groups>
            <run>
                <include name = "统计电子票"/>
                <include name = "统计专票"/>
                <include name="统计卷票"/>
                <include name="统计普票"/>
            </run>
        </groups>
        <classes>
            <class name = "com.shuzutech.invoiceStatistical.QueryInvoiceStatistical"/>
        </classes>
    </test>
    <listeners>
        <listener class-name="com.vimalselvam.testng.listener.ExtentTestNgFormatter"/>
        <listener class-name="com.shuzutech.config.ExtentTestNGIReporterListener"/>
    </listeners>


</suite>